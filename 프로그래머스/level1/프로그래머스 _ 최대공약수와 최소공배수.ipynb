   "outputs": [],
   "source": [
    "def solution(n, m):\n",
    "    gcd = max([y for y in [x for x in range(1,n+1) if not n%x] if not m%y])\n",
    "    return [gcd, n*m/gcd]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "2e14ad85",
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(n, m):\n",
    "    gcd = lambda a,b : b if not a%b else gcd(b, a%b)\n",
    "    lcm = lambda a,b : a*b//gcd(a,b)\n",
    "    return [gcd(n, m), lcm(n, m)]"
   ]
  },
