   "source": [
    "n=int(input())\n",
    "for _ in range(n):\n",
    "    s=input(); tmp = []\n",
    "    for x in s:\n",
    "        if not tmp:tmp.append(x)\n",
    "        elif (tmp[-1]==\"(\") and x == ')': tmp.pop()\n",
    "        else: tmp.append(x)\n",
    "    if tmp: print(\"NO\")\n",
    "    else: print(\"YES\")"
   ]
