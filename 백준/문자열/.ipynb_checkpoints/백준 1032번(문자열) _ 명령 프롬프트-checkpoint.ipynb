   "source": [
    "n=int(input())\n",
    "if n==1:print(input())\n",
    "else:\n",
    "    answer=''\n",
    "    for args in zip(*[input() for _ in range(n)]):\n",
    "        if len(set(args))==1:\n",
    "            answer+=args[0]\n",
    "        else: answer+='?'\n",
    "    print(answer)"
   ]
