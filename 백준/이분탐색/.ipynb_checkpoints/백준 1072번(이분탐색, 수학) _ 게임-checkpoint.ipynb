    "import math\n",
    "x,y = map(int,input().split())\n",
    "z = 100*y//x\n",
    "if z>=99:\n",
    "    print(-1)\n",
    "else:\n",
    "    k = (x*(z+1)-100*y)/(99-z)\n",
    "    print(math.ceil(k))"
