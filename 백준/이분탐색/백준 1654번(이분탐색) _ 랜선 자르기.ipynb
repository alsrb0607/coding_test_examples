   "source": [
    "k,n = map(int,input().split())\n",
    "s = [int(input()) for _ in range(k)]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "337cefe5",
   "metadata": {
    "scrolled": true
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1443\n"
     ]
    }
   ],
   "source": [
    "lo = 1; hi = max(s)\n",
    "while lo<=hi:\n",
    "    mid = (lo+hi)//2\n",
    "    t = sum(map(lambda x: x//mid, s))\n",
    "    if n>t:\n",
    "        hi = mid - 1\n",
    "    elif n<=t:\n",
    "        lo = mid + 1\n",
    "print(hi)"
   ]
