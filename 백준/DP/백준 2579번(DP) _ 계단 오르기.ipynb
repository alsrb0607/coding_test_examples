   "source": [
    "n=int(input())\n",
    "dp = [[0,0] for _ in range(n+1)]\n",
    "for i in range(1,n+1):\n",
    "    a=int(input())\n",
    "    if i==1:\n",
    "        dp[i] = [a,0]\n",
    "    else:\n",
    "        dp[i] = [max(dp[i-2][1]+a, dp[i-2][0]+a), dp[i-1][0]+a]\n",
    "print(max(dp[-1]))"
   ]
  },
