   "source": [
    "n = int(input())\n",
    "dp = [[0]*3]*2\n",
    "for _ in range(n):\n",
    "    a,b,c = map(int,input().split())\n",
    "    dp[0] = [a+min(dp[0][:2]), b+min(dp[0]), c+min(dp[0][1:])]\n",
    "    dp[1] = [a+max(dp[1][:2]), b+max(dp[1]), c+max(dp[1][1:])]\n",
    "print(max(dp[1]),min(dp[0]), end = ' ')"
   ]
  },
