   "source": [
    "n=int(input())\n",
    "dp = [0]*16\n",
    "for i in range(1,n+1):\n",
    "    a,b = map(int, input().split())\n",
    "    if i+a-1<=n:\n",
    "        dp[i+a-1]=max((max(dp[:i]) + b), dp[i+a-1])\n",
    "print(max(dp))\n"
   ]
