   "source": [
    "# dp로\n",
    "n=int(input())\n",
    "dp = [0,1]\n",
    "for _ in range(n-1):\n",
    "    dp = [dp[1],dp[0]+dp[1]]\n",
    "print(dp[0] if n==0 else dp[1])"
   ]
