   "source": [
    "# 재귀로\n",
    "n=int(input())\n",
    "def f(n):\n",
    "    if n==0: return 0\n",
    "    elif n==1: return 1\n",
    "    else: return f(n-1)+f(n-2)\n",
    "print(f(n))"
   ]
  },
