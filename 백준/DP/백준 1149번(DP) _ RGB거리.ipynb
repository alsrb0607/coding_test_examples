   "source": [
    "n=int(input())\n",
    "dp=[[0,0,0] for _ in range(n+1)]\n",
    "for k in range(1,n+1):\n",
    "    tmp= list(map(int, input().split()))\n",
    "    for i in range(3):\n",
    "        tmp[i] += min(dp[k-1][i-1], dp[k-1][i-2])\n",
    "    dp[k] = tmp\n",
    "print(min(dp[-1]))"
   ]
