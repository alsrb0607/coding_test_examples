   "source": [
    "n,s = map(int, input().split())\n",
    "a = list(map(int, input().split()))\n",
    "\n",
    "j=0\n",
    "tmp = 0\n",
    "ans = int(1e9)\n",
    "for i in range(n):\n",
    "    while j<n and tmp < s:\n",
    "        tmp += a[j]\n",
    "        j+=1\n",
    "    if tmp>=s:\n",
    "        ans = min(ans, j-i)\n",
    "    tmp -= a[i]\n",
    "print(ans)"
   ]
